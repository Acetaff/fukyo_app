<div class="contents row">
    <% @fukyos.each do |fukyo| %>
      <%= link_to fukyo_path(fukyo.id) do %>
        <%= image_tag fukyo.image.variant(resize: '500x500') %>
      <% end %>
        <h1>＃：<%= fukyo.title %></h1>
        <span class="name">
          <div>＃：<%= fukyo.f_text %></div>
        </span>
  <% end %>
</div>
